// Comprehensive test for std.io module
import std

func main():int {
    // Test string printing functions
    std.io.print("Hello ")
    std.io.println("World!")
    
    // Test integer printing
    let num:int = 42
    std.io.print("Number: ")
    std.io.println(num)
    
    // Test float printing
    let float_num:float = 3.14159
    std.io.print("Float: ")
    std.io.println(float_num)
    
    // Test boolean printing
    let bool_val:bool = true
    std.io.print("Boolean: ")
    std.io.println(bool_val)
    
    // Test mixed output
    std.io.print("Mixed: ")
    std.io.print(100)
    std.io.print(" + ")
    std.io.print(0.5)
    std.io.print(" = ")
    std.io.println(100.5 - 0.5)
    
    // Return 0 to indicate success
    return 0
}
