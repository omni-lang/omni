import std

func main(): int {
    let a = 12  // 1100 in binary
    let b = 10  // 1010 in binary
    
    // Test bitwise AND
    let and_result = a & b
    std.io.print("12 & 10 = ")
    std.io.println(and_result)  // Expected: 8 (1000)
    
    // Test bitwise OR
    let or_result = a | b
    std.io.print("12 | 10 = ")
    std.io.println(or_result)  // Expected: 14 (1110)
    
    // Test bitwise XOR
    let xor_result = a ^ b
    std.io.print("12 ^ 10 = ")
    std.io.println(xor_result)  // Expected: 6 (0110)
    
    // Test left shift
    let lshift_result = a << 2
    std.io.print("12 << 2 = ")
    std.io.println(lshift_result)  // Expected: 48 (110000)
    
    // Test right shift
    let rshift_result = a >> 2
    std.io.print("12 >> 2 = ")
    std.io.println(rshift_result)  // Expected: 3 (0011)
    
    // Test bitwise NOT
    let not_result = ~a
    std.io.print("~12 = ")
    std.io.println(not_result)  // Expected: -13 (in two's complement)
    
    // Test precedence: bitwise AND should have higher precedence than OR
    let precedence_result = a & b | 1
    std.io.print("(12 & 10) | 1 = ")
    std.io.println(precedence_result)  // Expected: 9 (8 | 1)
    
    return 0
}
