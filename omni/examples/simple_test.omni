// Simple test to verify the testing framework works

func test_simple_math() -> int {
    std.test.start("Simple Math Test");
    
    let a = 2;
    let b = 3;
    let result = a + b;
    
    std.assert.eq(result, 5, "2 + 3 should equal 5");
    
    std.test.end("Simple Math Test", true);
    return 0;
}

func main() -> int {
    std.io.println("Running simple test...");
    test_simple_math();
    std.test.summary();
    return 0;
}
