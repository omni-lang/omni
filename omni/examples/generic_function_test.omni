// Test generic functions
import std.io as io

// Generic function
func max<T>(a: T, b: T): T {
    if a > b {
        return a
    }
    return b
}

func main(): int {
    let intMax: int = max(10, 20)
    let floatMax: float = max(3.14, 2.71)
    
    io.println("Max int: " + intMax)
    io.println("Max float: " + floatMax)
    
    return 0
}
