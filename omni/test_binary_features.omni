// Test all binary generation features
import std.io

func fibonacci(n: int): int {
    if n <= 1 {
        return n
    }
    return fibonacci(n - 1) + fibonacci(n - 2)
}

func main(): int {
    // Test basic arithmetic
    let a = 10
    let b = 20
    let sum = a + b
    
    // Test function calls
    let fib_result = fibonacci(7)
    
    // Test string operations
    let message = "Hello, OmniLang!"
    std.io.println(message)
    
    // Test complex expressions
    let result = (sum * 2) + fib_result
    
    std.io.println("Sum: " + sum)
    std.io.println("Fibonacci(7): " + fib_result)
    std.io.println("Final result: " + result)
    
    return result
}
